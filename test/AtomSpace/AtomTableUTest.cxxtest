	cout << "BEGIN TEST: resetStimulus" << endl;
	atoms = createSimpleGraph(atomSpace, "stim_reset");

	list = HandleEntry::fromHandleVector(&atoms[0],atoms.size());
	stimValue = 100;

	table->stimulateAtom(list, stimValue);
	table->resetStimulus();
	for (unsigned int i=0; i < atoms.size(); i++) {
	    const EconomicAttentionValue& av = (const EconomicAttentionValue&) atoms[i]->getAttentionValue();
	    TS_ASSERT_EQUALS(av.getStimulus(), 0);
	}

	delete list;
	cout << "END TEST: resetStimulus" << endl;
    }
    
};

